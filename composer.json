{
  "name": "beechit/backup-restore",
  "type": "typo3-cms-extension",
  "description": "A low-level tool to backup and restore a full TYPO3 installation",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^10.3",
    "helhum/typo3-console": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "BeechIt\\BackupRestore\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/backup-restore": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    },
    "typo3/cms": {
      "extension-key": "backup-restore"
    }
  }
}
